{
"env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": [
    "prettier", "@typescript-eslint"
  ],
  "parserOptions": {
    "sourceType": "module",
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2020
  },
  "rules": {
    "prettier/prettier": "error",
    "camelcase": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "always"
    ]
  }
}